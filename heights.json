[
  {
    "dataset": {
      "urls": [
        "file://(path to dataset)/(zoom level)/${x}/${z}.png"
      ],
      "projection": {
          "web_mercator": {
              "zoom": (zoom level)
          }
      },
      "resolution": 256,
      "blend": "CUBIC",
      "parse": {
          "divide": {
              "delegate": {
                  "add": {
                      "delegate": {
                          "from_int": {
                              "delegate": {
                                  "rgb_extract": {
                                      "delegate": {
                                          "parse_png": {}
                                      }
                                  }
                              }
                          }
                      },
                      "value": -8388608
                  }
              },
              "value": 256.0
          }
      }
    },
    "bounds": {
      "minX": (left side in EPSG:4326),
      "maxX": (right side in EPSG:4326),
      "minZ": (bottom side in EPSG:4326),
      "maxZ": (top side in EPSG:4326)
    },
    "zooms": {
        "min": 0,
        "max": 3
    },
    "priority": 100
  }
]